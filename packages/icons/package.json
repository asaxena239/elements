{
  "license": "MIT",
  "name": "@rent_avail/icons",
  "version": "0.0.5",
  "description": "Icon library for Avail Design System",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo 'run tests from the root directory'",
    "build": "rm -rf dist && babel --root-mode upward src --out-dir dist --copy-files",
    "build:src": "svgo -f ./svg --config svgo.config.js && rm -rf src/icons && node ./scripts/build.js",
    "prepublishOnly": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Patrick Krawczykowski <patrick@dreadful.design>"
  ],
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "cheerio": "^1.0.0-rc.6",
    "feather-icons": "^4.28.0",
    "html-minifier": "^4.0.0",
    "prettier-eslint": "^12.0.0",
    "svgo": "^2.3.0",
    "uppercamelcase": "^3.0.0"
  }
}

{
  "license": "MIT",
  "name": "@rent_avail/icons",
  "version": "0.0.1",
  "description": "Icon library for Avail Design System",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "esmodule": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo 'run tests from the root directory'",
    "build": "microbundle --jsx React.createElement",
    "build:src": "svgo -f ./icons --config svgo.config.js && rm -rf src/icons && node build.js",
    "watch": "microbundle watch --no-compress --jsx React.createElement",
    "prepare": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Patrick Krawczykowski <patrick@dreadful.design>"
  ],
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.6",
    "feather-icons": "^4.28.0",
    "html-minifier": "^4.0.0",
    "prettier-eslint": "^12.0.0",
    "svgo": "^2.3.0",
    "uppercamelcase": "^3.0.0"
  }
}

{
  "license": "MIT",
  "name": "@doors/icons",
  "version": "0.1.3",
  "description": "Icon package for doors",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo 'run tests from the root directory'",
    "build": "rm -rf dist && babel --root-mode upward src --out-dir dist --copy-files",
    "build:src": "svgo -f ./svg --config svgo.config.js && rm -rf src/icons && ts-node ./scripts/build.ts",
    "prepack": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Patrick Krawczykowski <patrick@dreadful.design>"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "cheerio": "^1.0.0-rc.10",
    "html-minifier": "^4.0.0",
    "microbundle": "^0.14.2",
    "svgo": "^2.8.0",
    "uppercamelcase": "^3.0.0"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  }
}

{
  "license": "MIT",
  "name": "@doors/utils",
  "version": "0.0.6",
  "description": "Utility package for doors",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.m.js",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "mb": "microbundle --jsx 'React.createElement' --jsxImportSource react --globals react/jsx-runtime=jsx",
    "test": "echo 'run tests from the root directory'",
    "build": "yarn mb",
    "watch": "yarn mb watch --no-compress",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "microbundle": "^0.15.0",
    "react": "^18.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Patrick Krawczykowski <patrick@dreadful.design>"
  ]
}
